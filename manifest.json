{
    "name": "MySkEnchannter",
    "version": "1.2",
    "description": "",
    "manifest_version": 3,
    "permissions": ["activeTab", "storage", "scripting"],
   "icons": {
       "48": "/image/favicon.png"
       },
    "action": {
        "default_popup": "/popup/popup.html",
        "default_icon": "/image/favicon.png"
      },
      "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://www.mysk.school/*"
          ]
        }
      ],
      "background": {
        "service_worker": "/scripts/background.js"
      }
  }