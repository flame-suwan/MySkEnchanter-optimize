{
    "name": "MySkEnchannter",
    "version": "1.1",
    "description": "",
    "manifest_version": 3,
    "permissions": [
        "activeTab"
      ],
   "icons": {
       "48": "/image/favicon.png"
       },
    "action": {
        "default_popup": "/popup/popup.html",
        "default_icon": "/image/favicon.png"
      },
      "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": [
            "https://www.mysk.school/*"
          ]
        }
      ]
  }